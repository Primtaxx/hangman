is.startTime = Date.now();\n      this.timer = setInterval(() => {}, 1000);\n    },\n  },\n  mounted() {\n    window.addEventListener(\"keydown\", (e) => {\n      if (e.key.length === 1) {\n        this.guessLetter(e.key.toUpperCase());\n      }\n    });\n    this.startTimer();\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}