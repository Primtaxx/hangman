Letters() {\n      return this.letters;\n    },\n  },\n  watch: {\n    usedLetters() {\n      console.log(\"usedLetters changed!\");\n    },\n  },\n\n\n  methods: {\n    guessLetter(l) {\n  if (!this.won && !this.lost) {\n    if (this.letter) {\n      l = this.letter;\n      this.letter = \"\";\n    }\n    if (this.usedLetters.includes(l)) return;\n    this.usedLetters.push(l);\n    if (!this.word.includes(l)) {\n      this.guessesLeft--;\n      this.fehler--;\n    } else {\n      for (let i = 0; i < this.word.length; i++) {\n        if (this.word[i] === l) {\n          this.$set(this.maskedWord, i, l);\n        }\n      }\n    }\n    this.letters = this.letters.filter((letter) => letter !== l);\n    this.$forceUpdate();\n    this.checkGameOver();\n  }\n},\n\n\n\n\n\n\n\n\n\n\ncheckGameOver() {\n  if (!this.maskedWord.includes(\"_\")) {\n    this.won = true;\n    clearInterval(this.timer);\n    this.endTime = Date.now();\n    this.saveScore();\n  }\n  if (this.guessesLeft === 0) {\n    this.lost = true;\n    clearInterval(this.timer);\n  }\n},\n    saveScore() {\n      let scores = JSON.parse(localStorage.getItem(\"scores\")) || [];\n      let time = this.endTime - this.startTime;\n      scores.push({ time, word: this.word });\n      localStorage.setItem(\"scores\", JSON.stringify(scores));\n    },\n    cheat() {\n      this.show = true;\n    },\n    startTimer() {\n      this.startTime = Date.now();\n      this.timer = setInterval(() => {}, 1000);\n    },\n  },\n  mounted() {\n    window.addEventListener(\"keydown\", (e) => {\n      if (e.key.length === 1) {\n        this.guessLetter(e.key.toUpperCase());\n      }\n    });\n    this.startTimer();\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}