LowerCase());\n\n    if (this.word.includes(l.toLowerCase())) {\n      \n      let newMaskedWord = \"\";\n      for (let i = 0; i < this.word.length; i++) {\n        if (this.word[i] == l.toLowerCase()) {\n          newMaskedWord += l.toLowerCase();\n        } else {\n          newMaskedWord += \"_\";\n        }\n      }\n     \n      this.maskedWord = newMaskedWord;\n      this.gefunden += l.toLowerCase();\n      \n    }else {\n      \n      this.guessesLeft--;\n      // console.log(this.maskedWord)\n    }\n    this.letters = this.letters.filter((letter) => letter !== l);\n    this.$forceUpdate();\n    this.checkGameOver();\n  },\n\n\n  saveHighscore(){\n      localStorage.setItem(new Date().toLocaleString(),this.highscore)\n    },\n\n\n\n\ncheckGameOver() {\n  if (!this.maskedWord.includes(\"_\")) {\n    this.won = true;\n    clearInterval(this.timer);\n    this.endTime = Date.now();\n    this.saveScore();\n  }\n  if (this.guessesLeft === 0) {\n    this.lost = true;\n    clearInterval(this.timer);\n  }\n},\n    saveHighscore(){\n      localStorage.setItem(new Date().toLocaleString(),this.highscore)\n    },\n\n    cheat() {\n      this.show = true;\n    },\n    startTimer() {\n      this.startTime = Date.now();\n      this.timer = setInterval(() => {}, 1000);\n    },\n  },\n  mounted() {\n    console.log(this.word)\n    \n    window.addEventListener(\"keydown\", (e) => {\n      if (e.key.length === 1) {\n        this.guessLetter(e.key.toUpperCase());\n      }\n    });\n    this.startTimer();\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}