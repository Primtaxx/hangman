{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport WordData from \"../Aufgabe/wordlist.json\";\nexport default {\n  data() {\n    return {\n      word: WordData[Math.floor(Math.random() * WordData.length)],\n      letter: \"\",\n      letters: Array.from(Array(26)).map((e, i) => String.fromCharCode(i + 65)),\n      usedLetters: [],\n      guessesLeft: 10,\n      timer: null,\n      startTime: null,\n      endTime: null,\n      show: false,\n      won: false,\n      lost: false\n    };\n  },\n  methods: {\n    guessLetter(l) {\n      if (this.letter) {\n        l = this.letter;\n        this.letter = \"\";\n      }\n      if (this.usedLetters.includes(l) || this.won || this.lost) return;\n      this.usedLetters.push(l);\n      if (!this.word.includes(l)) {\n        this.guessesLeft--;\n      } else {\n        let newMaskedWord = \"\";\n        for (let i = 0; i < this.word.length; i++) {\n          if (this.word[i] === l) {\n            newMaskedWord += l;\n          } else {\n            newMaskedWord += this.maskedWord[i];\n          }\n        }\n        this.maskedWord = newMaskedWord;\n        this.scoreManager(10);\n        this.gefunden += l.toLowerCase();\n        console.log(this.maskedWord);\n      }\n      this.letters = this.letters.filter(letter => letter !== l);\n      this.$forceUpdate();\n      this.checkGameOver();\n    },\n    checkGameOver() {\n      if (!this.maskedWord.includes(\"_\")) {\n        this.won = true;\n        clearInterval(this.timer);\n        this.endTime = Date.now();\n        this.saveScore();\n      }\n      if (this.guessesLeft === 0) {\n        this.lost = true;\n        clearInterval(this.timer);\n      }\n    },\n    saveScore() {\n      let scores = JSON.parse(localStorage.getItem(\"scores\")) || [];\n      let time = this.endTime - this.startTime;\n      scores.push({\n        time,\n        word: this.word\n      });\n      localStorage.setItem(\"scores\", JSON.stringify(scores));\n    },\n    cheat() {\n      this.show = true;\n    },\n    startTimer() {\n      this.startTime = Date.now();\n      this.timer = setInterval(() => {}, 1000);\n    }\n  },\n  mounted() {\n    window.addEventListener(\"keydown\", e => {\n      if (e.key.length === 1) {\n        this.guessLetter(e.key.toUpperCase());\n      }\n    });\n    this.startTimer();\n  }\n};","map":{"version":3,"mappings":";AAiBA,OAAOA,QAAO,MAAO,0BAA0B;AAG/C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAEF,QAAQ,CAACG,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAIL,QAAQ,CAACM,MAAM,CAAC,CAAC;MAC3DC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAEC,KAAK,CAACC,IAAI,CAACD,KAAK,CAAC,EAAE,CAAC,CAAC,CAACE,GAAG,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKC,MAAM,CAACC,YAAY,CAACF,IAAI,EAAE,CAAC,CAAC;MACzEG,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE,KAAK;MACXC,GAAG,EAAE,KAAK;MACVC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EAIDC,OAAO,EAAE;IACPC,WAAW,CAACC,CAAC,EAAE;MACf,IAAI,IAAI,CAACnB,MAAM,EAAE;QACfmB,IAAI,IAAI,CAACnB,MAAM;QACf,IAAI,CAACA,MAAK,GAAI,EAAE;MAClB;MAEA,IAAI,IAAI,CAACS,WAAW,CAACW,QAAQ,CAACD,CAAC,KAAK,IAAI,CAACJ,GAAE,IAAK,IAAI,CAACC,IAAI,EAAE;MAC3D,IAAI,CAACP,WAAW,CAACY,IAAI,CAACF,CAAC,CAAC;MAExB,IAAI,CAAC,IAAI,CAACxB,IAAI,CAACyB,QAAQ,CAACD,CAAC,CAAC,EAAE;QAC1B,IAAI,CAACT,WAAW,EAAE;MACpB,OAAO;QACL,IAAIY,aAAY,GAAI,EAAE;QACtB,KAAK,IAAIhB,IAAI,CAAC,EAAEA,IAAI,IAAI,CAACX,IAAI,CAACI,MAAM,EAAEO,CAAC,EAAE,EAAE;UACzC,IAAI,IAAI,CAACX,IAAI,CAACW,CAAC,MAAMa,CAAC,EAAE;YACtBG,aAAY,IAAKH,CAAC;UACpB,OAAO;YACLG,aAAY,IAAK,IAAI,CAACC,UAAU,CAACjB,CAAC,CAAC;UACrC;QACF;QACA,IAAI,CAACiB,UAAS,GAAID,aAAa;QAC/B,IAAI,CAACE,YAAY,CAAC,EAAE,CAAC;QACrB,IAAI,CAACC,QAAO,IAAKN,CAAC,CAACO,WAAW,EAAE;QAChCC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACL,UAAU;MAC7B;MAEA,IAAI,CAACtB,OAAM,GAAI,IAAI,CAACA,OAAO,CAAC4B,MAAM,CAAE7B,MAAM,IAAKA,MAAK,KAAMmB,CAAC,CAAC;MAC5D,IAAI,CAACW,YAAY,EAAE;MACnB,IAAI,CAACC,aAAa,EAAE;IACtB,CAAC;IAMHA,aAAa,GAAG;MACd,IAAI,CAAC,IAAI,CAACR,UAAU,CAACH,QAAQ,CAAC,GAAG,CAAC,EAAE;QAClC,IAAI,CAACL,GAAE,GAAI,IAAI;QA